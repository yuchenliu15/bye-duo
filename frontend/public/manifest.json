{
  "manifest_version": 2,

  "name": "Bye DUO",
  "description": "Log in to NYU using just your browser! Goodbye to DUO!",
  "version": "0.0.1.5",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "128": "ByeDUO128.png",
    "48": "ByeDUO48.png",
    "16": "ByeDUO16.png"
  },
    "permissions": [
      "storage"
  ]
}
